<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">


    <Button
        android:id="@+id/button_go_to_entrant_edit_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go to Entrant Edit Profile" />
    <Button
        android:id="@+id/button_go_to_entrant_notifications"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go to Entrant Notifications" />
    <Button
        android:id="@+id/button_go_to_entrant_event_list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go to Entrant Event List" />

</LinearLayout>
    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/user_bottom_menu" />
</androidx.constraintlayout.widget.ConstraintLayout>





<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/button_go_to_entrant_edit_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go to Entrant Edit Profile" />
    <Button
        android:id="@+id/button_go_to_entrant_notifications"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go to Entrant Notifications" />
    <Button
        android:id="@+id/button_go_to_entrant_event_list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go to Entrant Event List" />
    <Button
        android:id="@+id/button_go_to_home_from_entrant_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go to Home" />

</LinearLayout>
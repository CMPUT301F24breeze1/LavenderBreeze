<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/home">

    <fragment
        android:id="@+id/home"
        android:name="com.example.myapplication.home"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_home_to_org_events_lst"
            app:destination="@id/org_events_lst" />
    </fragment>
    <fragment
        android:id="@+id/org_events_lst"
        android:name="com.example.myapplication.org_events_lst"
        android:label="fragment_org_events_lst"
        tools:layout="@layout/fragment_org_events_lst" >
        <action
            android:id="@+id/action_org_events_lst_to_home"
            app:destination="@id/home" />
        <action
            android:id="@+id/action_org_events_lst_to_org_event"
            app:destination="@id/org_event" />
        <action
            android:id="@+id/action_org_events_lst_to_org_add_event"
            app:destination="@id/org_add_event" />
        <action
            android:id="@+id/action_org_events_lst_to_org_edit_organizer"
            app:destination="@id/org_edit_organizer" />
    </fragment>
    <fragment
        android:id="@+id/org_event"
        android:name="com.example.myapplication.org_event"
        android:label="fragment_org_event"
        tools:layout="@layout/fragment_org_event" >
        <action
            android:id="@+id/action_org_event_to_org_view_event_qrcode"
            app:destination="@id/org_view_event_qrcode" />
        <action
            android:id="@+id/action_org_event_to_org_edit_event2"
            app:destination="@id/org_edit_event" />
        <action
            android:id="@+id/action_org_event_to_org_event_waiting_lst"
            app:destination="@id/org_event_waiting_lst" />
        <action
            android:id="@+id/action_org_event_to_org_event_selected_lst"
            app:destination="@id/org_event_selected_lst" />
    </fragment>
    <fragment
        android:id="@+id/org_edit_event"
        android:name="com.example.myapplication.org_edit_event"
        android:label="fragment_org_edit_event"
        tools:layout="@layout/fragment_org_edit_event" />
    <fragment
        android:id="@+id/org_view_event_qrcode"
        android:name="com.example.myapplication.org_view_event_qrcode"
        android:label="fragment_org_view_event_qrcode"
        tools:layout="@layout/fragment_org_view_event_qrcode" />
    <fragment
        android:id="@+id/org_event_waiting_lst"
        android:name="com.example.myapplication.org_event_waiting_lst"
        android:label="fragment_org_event_waiting_lst"
        tools:layout="@layout/fragment_org_event_waiting_lst" >
        <action
            android:id="@+id/action_org_event_waiting_lst_to_org_event_selected_lst"
            app:destination="@id/org_event_selected_lst" />
        <action
            android:id="@+id/action_org_event_waiting_lst_to_org_notif_waiting_lst"
            app:destination="@id/org_notif_waiting_lst" />
        <action
            android:id="@+id/action_org_event_waiting_lst_to_org_map"
            app:destination="@id/org_map" />
    </fragment>
    <fragment
        android:id="@+id/org_event_selected_lst"
        android:name="com.example.myapplication.org_event_selected_lst"
        android:label="fragment_org_event_selected_lst"
        tools:layout="@layout/fragment_org_event_selected_lst" />
    <fragment
        android:id="@+id/org_add_event"
        android:name="com.example.myapplication.org_add_event"
        android:label="fragment_org_add_event"
        tools:layout="@layout/fragment_org_add_event" />
    <fragment
        android:id="@+id/org_notif_waiting_lst"
        android:name="com.example.myapplication.org_notif_waiting_lst"
        android:label="fragment_org_notif_waiting_lst"
        tools:layout="@layout/fragment_org_notif_waiting_lst" />
    <fragment
        android:id="@+id/org_map"
        android:name="com.example.myapplication.org_map"
        android:label="fragment_org_map"
        tools:layout="@layout/fragment_org_map" />
    <fragment
        android:id="@+id/org_edit_organizer"
        android:name="com.example.myapplication.org_edit_organizer"
        android:label="fragment_org_edit_organizer"
        tools:layout="@layout/fragment_org_edit_organizer" />


</navigation>